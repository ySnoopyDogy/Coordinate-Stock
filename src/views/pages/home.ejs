<% contentFor('body') %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nome</th>
      <th scope="col">Descrição</th>
      <th scope="col">Mundo</th>
      <th scope="col">X</th>
      <th scope="col">Y</th>
      <th scope="col">Z</th>
    </tr>
  </thead>
  <tbody>
    <% table.forEach( tb => { %>
    <tr>
      <td><%=tb.id%></td>
      <td><%=tb.coord_name%></td>
      <td><%=tb.coord_description%></td>
      <td><%=tb.coord_world%></td>
      <td><%=tb.x%></td>
      <td><%=tb.y%></td>
      <td><%=tb.z%></td>
    </tr>
    <% }) %>
  </tbody>
</table>
